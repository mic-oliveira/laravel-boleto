{
    "name": "michaelferreira/laravel-boleto",
    "license": "MIT",
    "authors": [
        {
            "name": "Michael de Oliveira Ferreira",
            "email": "oliveiramichael@outlook.com"
        }
    ],
    "repositories": [
        {
            "type": "path",
            "url": "./api-bradesco",
            "options": {
                "symlink": true
            }
        }
    ],
    "require": {
        "php": "^7.4",
      "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.13",
        "laravel/legacy-factories": "^1.0.4"
    },
    "autoload": {
        "psr-4": {
            "Boleto\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Boleto\\": "./src",
            "Boleto\\Tests\\": "./tests"
        }
    },
    "laravel": {
        "providers": [
            "Boleto\\Provider\\ServiceProvider"
        ]
    }
}
